<?xml version="1.0"?>
<aiml>
  <!-- processo de aprendizado -->
  <category>
    <pattern>* * *</pattern>
    <template><addtriple><subj><lowercase><star index="1"/></lowercase></subj><pred><lowercase><star index="2"/></lowercase></pred><obj><lowercase><star index="3"/></lowercase></obj></addtriple>
            Entendido, aprendi.
        </template>
  </category>
  <category>
    <pattern>tudo</pattern>
    <template>
            sei tudo isso:
            <lowercase><select><vars>?x ?z ?y</vars><q><subj>?x</subj><pred>?z</pred><obj>?y</obj></q></select></lowercase>
        </template>
  </category>
  <category>
    <pattern>quem * *</pattern>
    <template>
      <think>
        <set var="tuples">
          <select>
            <vars>?y</vars>
            <q>
              <subj>
                <star index="1"/>
              </subj>
              <pred>
                <star index="2"/>
              </pred>
              <obj>?y</obj>
            </q>
          </select>
        </set>
      </think>
      <lowercase>
        <get var="?y">
          <tuple>
            <get var="tuples"/>
          </tuple>
        </get>
      </lowercase>
    </template>
  </category>
</aiml>
