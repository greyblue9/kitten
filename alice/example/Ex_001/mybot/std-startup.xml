        <!-- Pattern用来匹配用户输入 -->
        <!-- 如果用户输入 "LOAD AIML B" -->
        <pattern>LOAD AIML C</pattern>
        <!-- Template是模式的响应 -->
        <!-- 这里学习一个aiml文件 -->
        <template>
            <!--learn>basic_chat.aiml</learn> -->
            <learn>dev-webhelper.aiml</learn>
            <!-- 你可以在这里添加更多的aiml文件 -->
            <!--<learn>more_aiml.aiml</learn>-->
            <learn>tuling.aiml</learn>
        </template>
    </category>
</aiml>